  <tr>
    <th class="news_byte_th" align="left">
    <div><div class="news_byte_title"><%=h @news_byte.title %></div>
    <div class="news_byte_time"><%=h time_ago_in_words @news_byte.updated_at %> ago</div></div>
    </th>
  </tr>
  <tr>
    <td class="news_byte"><%= raw RedCloth.new( @news_byte.body ).to_html %></td>
    <% if @logged_in %>
    <td><%= link_to 'Show', @news_byte %></td>
    <td><%= link_to 'Edit', edit_news_byte_path(@news_byte) %></td>
    <td><%= link_to 'Destroy', @news_byte, :confirm => 'Are you sure?', :method => :delete %></td>
    <% end %>
  </tr>
  <tr>
    <th align="left">&nbsp;</th>
  </tr>
